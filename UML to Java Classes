public class Student{
    private string Name;
    
    public string getName(){
    return this.Name;
    }
    
    public void setName(string name){
    this.Name = name;
    }

    private int Id;
    
    public int getId(){
    return this.Id;
    }
    
    public void setId(int id){
    this.Id = id;
    }
}
